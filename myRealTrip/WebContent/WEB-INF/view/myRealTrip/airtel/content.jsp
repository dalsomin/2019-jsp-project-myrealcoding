		}
	    ,error:function(){
	    	alert("에러~~~~")
	    } 
	});
}

function nextMonth(){
	// console.log("다음달");
	
	if(c_year == new Date().getYear()+1900+1 && c_month == new Date().getMonth()){
		return;
	}
	
	c_month  = c_month+1;
	if(c_month == 13){
		c_month = 1;
		c_year = c_year+1;
	}
	$(".cal_header1").html("");
	$(".cal_table1 tbody").remove();

	$.ajax({
		url:"<%= request.getContextPath()%>/flights/printCalendar.do"
		,dataType:"json"
		,type:"get"
		,data: {
			c_month: c_month,
			c_year: c_year
		}
		,cache:false // 항상 새로 읽어오겠다.
		,success:function(data){
			printCalendar(data);
		}
	    ,error:function(){
	    	alert("에러~~~~")
	    } 
	});
}
</script>



