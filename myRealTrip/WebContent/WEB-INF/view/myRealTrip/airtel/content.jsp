<script>
$('.option-plus').on("click",function(){
	var cnt = Number($(this).prev().text()) + 1;
	$(this).prev().text(cnt);
	if(cnt==1){
		$(this).prev().prev().removeClass("option-disable").addClass("option-clicked");
		$(this).prev().prev().css("pointer-events","all");
	}
	if(cnt==10){
		$(this).removeClass("option-clicked").addClass("option-disable");
		$(this).css("pointer-events","none");
	}
	
})

$('.option-minus').on("click",function(){
	var cnt = Number($(this).next().text()) -1;
	$(this).next().text(cnt);
	if(cnt==0){
		$(this).removeClass("option-clicked").addClass("option-disable");
		$(this).css("pointer-events","none");
	}
	if(cnt==9){
		$(this).next().next().removeClass("option-disable").addClass("option-clicked");
		$(this).next().next().css("pointer-events","all");
	}
	
})

</script>

